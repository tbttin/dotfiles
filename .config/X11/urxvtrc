! vim: filetype=xdefaults commentstring=!%s

! Disable Perl extentions.
URxvt.perl-ext:
URxvt.perl-ext-common:

! Hide scrollbar.
URxvt.scrollBar: false

! Disable Ctrl+Shift.
! You have disabled keycap picture insert mode but not basic ISO 14755 mode. To disable both, set both to false.
URxvt.iso14755: false
URxvt.iso14755_52: false

! Disable mouse scroll.
URxvt.saveLines: 0

! What is exactly bold?
URxvt.intensityStyles: false
